{
  "version": 3,
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api/broken$",
      "dest": "/api/broken",
      "check": true
    },
    {
      "src": "^/api/edge$",
      "dest": "/api/edge",
      "check": true
    },
    {
      "src": "^/api/works$",
      "dest": "/api/works",
      "check": true
    }
  ],
  "overrides": {}
}